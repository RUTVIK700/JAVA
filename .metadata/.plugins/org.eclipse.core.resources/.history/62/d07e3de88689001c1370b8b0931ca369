
public class ArrayOutBound {

	public static void main(String[] args) {
	try {
		int arr[]= {10,20,30};
		for(int i=0;i<=arr.length;i++){
			System.out.println(arr[i]);
		}
	} catch (ArrayIndexOutOfBoundsException e) {
		System.out.println("ASSHOLE");
	}
	}
	public void method() {
		System.out.println("hi");
	}

}
